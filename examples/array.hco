let i = 0;
let list = array();

/* Add items to the hash table */
while (i < 10)
{
    list[i] = ("Item " + i);
    i++;
}

/* Print items */
let i = 0;
while (i < 10)
{
    print(list[i]);
    i++;
}

print("Array count: " + count(list));



let i = 1;
let maxNumber = 15;
while (i < maxNumber)
{
    if (i % 5 == 0 && i % 3 == 0)
    {
        printLn("FizzBuzz");
    }
    else if (i % 3 == 0)
    {
        printLn("Fizz");
    }
    else if (i % 5 == 0)
    {
        printLn("Buzz");
    }
    else
    {
        printLn(i);
    }
    i++;
}

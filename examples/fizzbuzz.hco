set i = 1;
set maxNumber = 15;
while (i < maxNumber)
{
    if (i % 5 == 0 && i % 3 == 0)
    {
        print("FizzBuzz");
    }
    else if (i % 3 == 0)
    {
        print("Fizz");
    }
    else if (i % 5 == 0)
    {
        print("Buzz");
    }
    else
    {
        print(i);
    }
    set i = i + 1;
}
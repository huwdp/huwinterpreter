function isLeapYear(year)
{
    if (year % 4 == 0)
    {
        return true;
    }
    if (year % 100 == 0)
    {
        return true;
    }
    if (year % 400 == 0)
    {
        return true;
    }
    return false;
}

let i = 0;
while (i < 2100)
{
    let isLeap = isLeapYear(i);
    if (isLeap == true)
    {
        print(str(i) + " is a leap year");
    }
    else
    {
       print(str(i) + " is not a leap year");
    }
    i++;
}
